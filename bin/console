#!/usr/bin/env ruby
# frozen_string_literal: true

require 'bundler/setup'
require 'robot_v2'

new_robot = RobotV2::Board.new
new_robot.create_board(5)
new_robot.display_board(0, 0)
new_robot.display_board(4, 4)
new_robot.display_board(2, 2)








